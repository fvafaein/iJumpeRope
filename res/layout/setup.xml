<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back"
    android:orientation="vertical" >

    <Space
        android1:layout_width="match_parent"
        android1:layout_height="wrap_content" />

    <TextView
        android1:id="@+id/textView1"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_gravity="center"
        android1:layout_marginTop="60dp"
        android:textColor="#ffffff"
        android1:text="Settings"
        android1:textSize="30dp" />

    <TextView
        android1:id="@+id/textView2"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_gravity="center"
        android1:layout_marginTop="30dp"
        android:textColor="#ffffff"
        android1:text="Phone Orientation When Jumping:"
        android1:textSize="20sp" />

    <RadioGroup
        android1:id="@+id/radioGroup1"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_gravity="center" >

        <RadioButton
            android1:id="@+id/radio0"
            android1:layout_width="wrap_content"
            android1:layout_height="wrap_content"
            android1:checked="true"
            android1:text="Upwards" />

        <RadioButton
            android1:id="@+id/radio1"
            android1:layout_width="wrap_content"
            android1:layout_height="wrap_content"
            android1:text="Downwards" />
    </RadioGroup>

    <EditText
        android1:id="@+id/editText1"
        android1:layout_width="160dp"
        android1:layout_height="wrap_content"
        android1:ems="10"
        android1:layout_gravity="center"
        android1:layout_marginTop="20dp"
        android1:hint="Weight in pounds"
        android1:inputType="number" />

    <TextView
        android1:id="@+id/textView3"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_gravity="center"
        android1:layout_marginTop="30dp"
        android:textColor="#ffffff"
        android1:text="-   Jump Sensitivity   +"
        android1:textSize="20sp" />

    <SeekBar
        android1:id="@+id/seekBar1"
        android1:layout_width="200dp"
        android1:layout_height="wrap_content"
        android1:layout_gravity="center_horizontal"
        android:max="100"
        android:progress="50" />

    <TextView
        android1:id="@+id/textView4"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_gravity="center"
        android1:layout_marginTop="30dp"
        android:textColor="#ffffff"
        android1:text="Run Test"
        android1:textSize="20sp" />

    <ToggleButton
        android1:id="@+id/toggleButton1"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_gravity="center"
        android1:text="ToggleButton" />

</LinearLayout>